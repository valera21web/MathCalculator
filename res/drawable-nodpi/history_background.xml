<?xml version="1.0" encoding="utf-8"?>

<selector xmlns:android="http://schemas.android.com/apk/res/android">
   <item android:state_pressed="true">
      <shape>
         <gradient android:angle="90.0"
                   android:endColor="@color/backgroundColorHistoryListPressStart"
                   android:startColor="@color/backgroundColorHistoryListPressEnd"
                   android:type="linear" />
         <corners android:radius="0dp" />
      </shape>
   </item>
   <item>
      <shape>
         <gradient
                 android:angle="90.0"
                 android:endColor="@color/backgroundColorHistoryListStart"
                 android:startColor="@color/backgroundColorHistoryListEnd"
                 android:type="linear" />
         <corners android:radius="0dp" />
      </shape>
   </item>
</selector>