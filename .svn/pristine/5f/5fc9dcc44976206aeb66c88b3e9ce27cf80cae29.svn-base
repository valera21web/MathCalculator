<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
   android:layout_width="match_parent"
   android:layout_height="wrap_content"
   android:baselineAligned="true"
   android:id="@+id/layout"
   android:orientation="horizontal" >

   <TableLayout
      android:layout_width="fill_parent"
      android:layout_height="match_parent" android:background="@color/tableBackground" android:layout_weight="1">

      <TableRow
         android:id="@+id/tableRow11"
         android:layout_width="fill_parent"
         android:layout_height="@dimen/buttonKeyboardHeight">

         <Button
            android:id="@+id/buttonE"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:width="@dimen/buttonKeyboardWidth"
            android:height="@dimen/buttonKeyboardHeight"
            android:textSize="@dimen/buttonKeyboardWordsSizeText"
            android:background="@drawable/button_style_keyboard"
            android:textColor="@color/colorTextButtonKeyboard"
            android:layout_marginRight="1dp"
            android:text="E" />

         <Button
            android:id="@+id/buttonPi"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:width="@dimen/buttonKeyboardWidth"
            android:height="@dimen/buttonKeyboardHeight"
            android:textSize="@dimen/buttonKeyboardWordsSizeText"
            android:background="@drawable/button_style_keyboard"
            android:textColor="@color/colorTextButtonKeyboard"
            android:layout_marginRight="1dp"
            android:text="Pi" />

         <Button
                 android:id="@+id/buttonStep"
                 android:layout_width="match_parent"
                 android:layout_height="match_parent"
                 android:width="@dimen/buttonKeyboardWidth"
                 android:height="@dimen/buttonKeyboardHeight"
                 android:textSize="@dimen/buttonKeyboardWordsSizeText"
                 android:background="@drawable/button_style_keyboard"
                 android:textColor="@color/colorTextButtonKeyboard"
                 android:layout_marginRight="1dp"
                 android:text="^"/>

      </TableRow>

      <TableRow
         android:id="@+id/tableRow12"
         android:layout_width="fill_parent"
         android:layout_height="@dimen/buttonKeyboardHeight"
         android:layout_marginTop="1dp"
         android:layout_marginBottom="1dp">

         <Button
            android:id="@+id/button7"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:textSize="@dimen/buttonKeyboardNumberSizeText"
            android:background="@drawable/button_style_keyboard"
            android:width="@dimen/buttonKeyboardWidth"
            android:height="@dimen/buttonKeyboardHeight"
            android:textColor="@color/colorTextButtonKeyboard"
            android:layout_marginRight="1dp"
            android:text="7" />

         <Button
            android:id="@+id/button8"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:textSize="@dimen/buttonKeyboardNumberSizeText"
            android:background="@drawable/button_style_keyboard"
            android:layout_marginRight="1dp"
            android:width="@dimen/buttonKeyboardWidth"
            android:height="@dimen/buttonKeyboardHeight"
            android:textColor="@color/colorTextButtonKeyboard"
            android:text="8" />

         <Button
            android:id="@+id/button9"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:textSize="@dimen/buttonKeyboardNumberSizeText"
            android:background="@drawable/button_style_keyboard"
            android:layout_marginRight="1dp"
            android:width="@dimen/buttonKeyboardWidth"
            android:height="@dimen/buttonKeyboardHeight"
            android:textColor="@color/colorTextButtonKeyboard"
            android:text="9" />

      </TableRow>

      <TableRow
         android:id="@+id/tableRow3"
         android:layout_width="fill_parent"
         android:layout_height="@dimen/buttonKeyboardHeight">

         <Button
            android:id="@+id/button4"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:textSize="@dimen/buttonKeyboardNumberSizeText"
            android:background="@drawable/button_style_keyboard"
            android:layout_marginRight="1dp"
            android:width="@dimen/buttonKeyboardWidth"
            android:height="@dimen/buttonKeyboardHeight"
            android:textColor="@color/colorTextButtonKeyboard"
            android:text="4" />

         <Button
            android:id="@+id/button5"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:textSize="@dimen/buttonKeyboardNumberSizeText"
            android:background="@drawable/button_style_keyboard"
            android:layout_marginRight="1dp"
            android:width="@dimen/buttonKeyboardWidth"
            android:height="@dimen/buttonKeyboardHeight"
            android:textColor="@color/colorTextButtonKeyboard"
            android:text="5" />

         <Button
            android:id="@+id/button6"
            android:layout_width="match_parent"
            android:textSize="@dimen/buttonKeyboardNumberSizeText"
            android:background="@drawable/button_style_keyboard"
            android:layout_marginRight="1dp"
            android:paddingTop="@dimen/buttonKeyboard6PaddingTop"
            android:width="@dimen/buttonKeyboardWidth"
            android:height="@dimen/buttonKeyboardHeight"
            android:textColor="@color/colorTextButtonKeyboard"
            android:text="6" />

      </TableRow>

      <TableRow
         android:id="@+id/tableRow4"
         android:layout_width="fill_parent"
         android:layout_height="@dimen/buttonKeyboardHeight"
         android:layout_marginTop="1dp"
         android:layout_marginBottom="1dp" >

         <Button
            android:id="@+id/button1"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:textSize="@dimen/buttonKeyboardNumberSizeText"
            android:background="@drawable/button_style_keyboard"
            android:layout_marginRight="1dp"
            android:width="@dimen/buttonKeyboardWidth"
            android:height="@dimen/buttonKeyboardHeight"
            android:textColor="@color/colorTextButtonKeyboard"
            android:text="1" />

         <Button
            android:id="@+id/button2"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:textSize="@dimen/buttonKeyboardNumberSizeText"
            android:background="@drawable/button_style_keyboard"
            android:layout_marginRight="1dp"
            android:width="@dimen/buttonKeyboardWidth"
            android:height="@dimen/buttonKeyboardHeight"
            android:textColor="@color/colorTextButtonKeyboard"
            android:text="2" />

         <Button
            android:id="@+id/button3"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:textSize="@dimen/buttonKeyboardNumberSizeText"
            android:background="@drawable/button_style_keyboard"
            android:layout_marginRight="1dp"
            android:width="@dimen/buttonKeyboardWidth"
            android:height="@dimen/buttonKeyboardHeight"
            android:textColor="@color/colorTextButtonKeyboard"
            android:text="3"/>

      </TableRow>

      <TableRow
         android:id="@+id/tableRow5"
         android:layout_width="fill_parent"
         android:layout_height="fill_parent"
         android:height="@dimen/buttonKeyboardHeight">

         <Button
            android:id="@+id/buttonBracket"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:textSize="@dimen/buttonKeyboardOperatorsSizeText"
            android:background="@drawable/button_style_keyboard"
            android:layout_marginRight="1dp"
            android:width="@dimen/buttonKeyboardWidth"
            android:height="@dimen/buttonKeyboardHeight"
            android:textColor="@color/colorTextButtonKeyboard"
            android:text="()" />

         <Button
            android:id="@+id/button0"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:textSize="@dimen/buttonKeyboardNumberSizeText"
            android:background="@drawable/button_style_keyboard"
            android:layout_marginRight="1dp"
            android:width="@dimen/buttonKeyboardWidth"
            android:height="@dimen/buttonKeyboardHeight"
            android:textColor="@color/colorTextButtonKeyboard"
            android:text="0" />

         <Button
            android:id="@+id/buttonPoint"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:textSize="@dimen/buttonKeyboardOperatorsSizeText"
            android:background="@drawable/button_style_keyboard"
            android:layout_marginRight="1dp"
            android:width="@dimen/buttonKeyboardWidth"
            android:height="@dimen/buttonKeyboardHeight"
            android:textColor="@color/colorTextButtonKeyboard"
            android:text="," />


      </TableRow>
   </TableLayout>

   <TableLayout
           android:layout_width="fill_parent"
           android:layout_height="fill_parent"
           android:layout_weight="3" android:background="@color/tableBackground" android:paddingLeft="1dp">

      <TableRow
              android:id="@+id/tableRow21"
              android:layout_width="fill_parent"
              android:layout_height="fill_parent"
              android:height="@dimen/buttonKeyboardHeight">
         <Button
                 android:id="@+id/buttonDiv"
                 android:layout_width="match_parent"
                 android:layout_height="match_parent"
                 android:width="@dimen/buttonKeyboardWidth"
                 android:height="@dimen/buttonKeyboardHeight"
                 android:textSize="@dimen/buttonKeyboardOperatorsSizeText"
                 android:background="@drawable/button_style_keyboard"
                 android:textColor="@color/colorTextButtonKeyboard"
                 android:text="/" />
      </TableRow>
      <TableRow
              android:id="@+id/tableRow22"
              android:layout_width="fill_parent"
              android:layout_height="fill_parent"
              android:layout_marginTop="1dp"
              android:layout_marginBottom="1dp"
              android:height="@dimen/buttonKeyboardHeight">
         <Button
                 android:id="@+id/buttonMult"
                 android:layout_width="match_parent"
                 android:layout_height="match_parent"
                 android:textSize="@dimen/buttonKeyboardOperatorsSizeText"
                 android:background="@drawable/button_style_keyboard"
                 android:width="@dimen/buttonKeyboardWidth"
                 android:height="@dimen/buttonKeyboardHeight"
                 android:textColor="@color/colorTextButtonKeyboard"
                 android:text="*" />
      </TableRow>

      <TableRow
              android:id="@+id/tableRow23"
              android:layout_width="fill_parent"
              android:layout_height="fill_parent"
              android:height="@dimen/buttonKeyboardHeight">
         <Button
                 android:id="@+id/buttonMinus"
                 android:layout_width="match_parent"
                 android:layout_height="match_parent"
                 android:textSize="@dimen/buttonKeyboardOperatorsSizeText"
                 android:background="@drawable/button_style_keyboard"
                 android:width="@dimen/buttonKeyboardWidth"
                 android:height="@dimen/buttonKeyboardHeight"
                 android:textColor="@color/colorTextButtonKeyboard"
                 android:text="-" />
      </TableRow>

      <TableRow
              android:id="@+id/tableRow24"
              android:layout_width="fill_parent"
              android:layout_height="fill_parent"
              android:layout_marginTop="1dp"
              android:height="@dimen/buttonKeyboardHeightPlus">
         <Button
                 android:id="@+id/buttonPlus"
                 android:layout_width="match_parent"
                 android:layout_height="match_parent"
                 android:textSize="@dimen/buttonKeyboardOperatorsSizeText"
                 android:background="@drawable/button_style_keyboard"
                 android:width="@dimen/buttonKeyboardWidth"
                 android:height="@dimen/buttonKeyboardHeightPlus"
                 android:textColor="@color/colorTextButtonKeyboard"
                 android:text="+"/>
      </TableRow>
   </TableLayout>
</LinearLayout>