<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:baselineAligned="true"
    android:id="@+id/layout"
    android:orientation="horizontal" >
		<TableLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent" android:background="@color/tableBacground">

               <TableRow
                   android:id="@+id/tableRow1"
                   android:layout_width="match_parent"
                   android:layout_height="match_parent"
				   android:paddingLeft="1dp" 
				   android:paddingRight="1dp">
                <Button
                    android:id="@+id/buttonMS"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:width="@dimen/widht_button3"
                    android:height="@dimen/height_button3"
                    android:textSize="@dimen/button3_text_size"
                    android:background="@drawable/buttonstyle2" 
                    android:layout_marginRight="1dp"
                    android:text="MS"/>
                <Button
                    android:id="@+id/buttonMC"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:width="@dimen/widht_button3"
                    android:height="@dimen/height_button3"
                    android:textSize="@dimen/button3_text_size"
                    android:background="@drawable/buttonstyle2" 
                    android:layout_marginRight="1dp"
                    android:text="MC" />
                <Button
                    android:id="@+id/buttonMR"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:width="@dimen/widht_button3"
                    android:height="@dimen/height_button3"
                    android:textSize="@dimen/button3_text_size"
                    android:background="@drawable/buttonstyle2"  
                    android:layout_marginRight="1dp"
                    android:text="MR" />
                
                <Button
                    android:id="@+id/buttonStep"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:width="@dimen/widht_button3_1"
                    android:height="@dimen/height_button3"
                    android:textSize="@dimen/button3_text_size"
                    android:background="@drawable/buttonstyle2"
                    android:text="^" />
               </TableRow>

                <TableRow
                    android:id="@+id/tableRow2"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_margin="1dp" >
	                <Button
	                    android:id="@+id/button7"
	                    android:layout_width="match_parent"
	                    android:layout_height="match_parent"
	                    android:textSize="@dimen/button3_text_size_b"
	                    android:background="@drawable/buttonstyle2"  
	                    android:height="@dimen/height_button3_b"
	                    android:layout_marginRight="1dp"
	                    android:text="7" />
	                <Button
	                    android:id="@+id/button8"
	                    android:layout_width="match_parent"
	                    android:layout_height="match_parent"
	                    android:textSize="@dimen/button3_text_size_b"
	                    android:background="@drawable/buttonstyle2" 
	                    android:layout_marginRight="1dp" 
                   		android:height="@dimen/height_button3_b"
	                    android:text="8" />
	                <Button
	                    android:id="@+id/button9"
	                    android:layout_width="match_parent"
	                    android:layout_height="match_parent"
	                    android:textSize="@dimen/button3_text_size_b"
	                    android:background="@drawable/buttonstyle2" 
	                    android:layout_marginRight="1dp"
	                    android:height="@dimen/height_button3_b"
	                    android:text="9" />
	                <Button
	                    android:id="@+id/buttonDiv"
	                    android:layout_width="match_parent"
	                    android:layout_height="match_parent"
	                    android:textSize="@dimen/button3_text_size"
	                    android:background="@drawable/buttonstyle2" 
	                    android:height="@dimen/height_button3"
	                    android:text="/" />
                </TableRow>

                <TableRow
                    android:id="@+id/tableRow3"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:paddingLeft="1dp" 
					android:paddingRight="1dp">
	                <Button
	                    android:id="@+id/button4"
	                    android:layout_width="match_parent"
	                    android:layout_height="match_parent"
	                    android:textSize="@dimen/button3_text_size_b"
	                    android:background="@drawable/buttonstyle2" 
	                    android:layout_marginRight="1dp"
	                    android:height="@dimen/height_button3_b"
	                    android:text="4" />
	                <Button
	                    android:id="@+id/button5"
	                    android:layout_width="match_parent"
	                    android:layout_height="match_parent"
	                    android:textSize="@dimen/button3_text_size_b"
	                    android:background="@drawable/buttonstyle2" 
	                    android:layout_marginRight="1dp"
	                    android:height="@dimen/height_button3_b"
	                    android:text="5" />
	                <Button
	                    android:id="@+id/button6"
	                    android:layout_width="match_parent"
	                    android:textSize="@dimen/button3_text_size_b"
	                    android:background="@drawable/buttonstyle2" 
	                    android:layout_marginRight="1dp"
	                    android:height="@dimen/height_button3_b"
	                    android:text="6" />
	                <Button
	                    android:id="@+id/buttonMult"
	                    android:layout_width="match_parent"
	                    android:layout_height="match_parent"
	                    android:textSize="@dimen/button3_text_size"
	                    android:background="@drawable/buttonstyle2"
	                    android:height="@dimen/height_button3"
	                    android:text="*" />
                </TableRow>

                <TableRow
                    android:id="@+id/tableRow4"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_margin="1dp" >
	                <Button
	                    android:id="@+id/button1"
	                    android:layout_width="match_parent"
	                    android:layout_height="match_parent"
	                    android:textSize="@dimen/button3_text_size_b"
	                    android:background="@drawable/buttonstyle2" 
	                    android:layout_marginRight="1dp"
	                    android:height="@dimen/height_button3_b"
	                    android:text="1" />
	                <Button
	                    android:id="@+id/button2"
	                    android:layout_width="match_parent"
	                    android:layout_height="match_parent"
	                    android:textSize="@dimen/button3_text_size_b"
	                    android:background="@drawable/buttonstyle2" 
	                    android:layout_marginRight="1dp"
	                    android:height="@dimen/height_button3_b"
	                    android:text="2" />
	                <Button
	                    android:id="@+id/button3"
	                    android:layout_width="match_parent"
	                    android:layout_height="match_parent"
	                    android:textSize="@dimen/button3_text_size_b"
	                    android:background="@drawable/buttonstyle2" 
	                    android:layout_marginRight="1dp"
	                    android:height="@dimen/height_button3_b"
	                    android:text="3" />
	                <Button
	                    android:id="@+id/buttonMinus"
	                    android:layout_width="match_parent"
	                    android:layout_height="match_parent"
	                    android:textSize="@dimen/button3_text_size"
	                    android:background="@drawable/buttonstyle2"
	                    android:height="@dimen/height_button3"
	                    android:text="-" />
                </TableRow>
                
                <TableRow
                    android:id="@+id/tableRow5"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:height="65dp"
                    android:paddingLeft="1dp" 
					android:paddingRight="1dp">
	                <Button
	                    android:id="@+id/buttonBracket"
	                    android:layout_width="match_parent"
	                    android:layout_height="match_parent"
	                    android:textSize="@dimen/button3_text_size"
	                    android:background="@drawable/buttonstyle2" 
	                    android:layout_marginRight="1dp"
	                    android:height="@dimen/height_button3"
	                    android:text="()" />
	                <Button
	                    android:id="@+id/button0"
	                    android:layout_width="match_parent"
	                    android:layout_height="match_parent"
	                    android:textSize="@dimen/button3_text_size_b"
	                    android:background="@drawable/buttonstyle2" 
	                    android:layout_marginRight="1dp"
	                    android:height="@dimen/height_button3_b"
	                    android:text="0" />
	                <Button
	                    android:id="@+id/buttonPoint"
	                    android:layout_width="match_parent"
	                    android:layout_height="match_parent"
	                    android:textSize="@dimen/button3_text_size"
	                    android:background="@drawable/buttonstyle2" 
	                    android:layout_marginRight="1dp"
	                    android:height="@dimen/height_button3"
	                    android:text="," />
	                <Button
	                    android:id="@+id/buttonPlus"
	                    android:layout_width="match_parent"
	                    android:layout_height="match_parent"
	                    android:textSize="@dimen/button3_text_size"
	                    android:background="@drawable/buttonstyle2"
	                    android:height="@dimen/height_button3"
	                    android:text="+" />
                </TableRow>
            </TableLayout>
</LinearLayout>